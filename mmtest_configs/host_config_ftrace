# List of monitors for the host.
# why not just use config_ftrace? becuase i can specify different FTRACE_EVENTS for host and guest
export RUN_MONITOR=yes
export MONITORS_GZIP="ftrace"
export MONITOR_FTRACE_EVENTS="sched irq kvm/kvm_entry kvm/kvm_exit"
export MONITORS_ALWAYS=""
export MONITORS_WITH_LATENCY=""
export MONITORS_TRACER=""
export MONITOR_PERF_EVENTS=""
